{
  "name": "Frontify",
  "compatibleVersion": 3,
  "identifier": "com.frontify.sketch-plugin",
  "version": "0.1.0",
  "description": "Connect your Frontify account to Sketch.",
  "bundleVersion": 2,
  "authorEmail": "hello@frontify.com",
  "author": "Frontify AG",
  "commands": [
    {
      "name": "Artboards",
      "identifier": "artboards",
      "script": "./commands/frontify.js",
      "shortcut": "ctrl shift a",
      "handler": "artboardsCommand"
    },
    {
      "name": "Sources",
      "identifier": "sources",
      "script": "./commands/frontify.js",
      "shortcut": "ctrl shift s",
      "handler": "sourcesCommand"
    },
    {
      "name": "Saved",
      "identifier": "saved",
      "script": "./commands/frontify.js",
      "handlers": {
        "actions": {
          "DocumentSaved": "savedCommand"
        }
      }
    },
    {
      "name": "Init",
      "identifier": "init",
      "script": "./commands/frontify.js",
      "handlers": {
        "actions": {
          "OpenDocument": "initCommand"
        }
      }
    }
  ],
  "menu": {
    "items": [
      "artboards",
      "sources"
    ]
  }
}